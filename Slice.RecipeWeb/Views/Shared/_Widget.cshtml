@using SubjectEngine.Core
@using Slice.Web.Models
@model DynamicWidgetViewModel

@foreach (DucViewModel subitem in Model.Ducs)
{
  if (subitem.DucType != DucTypes.Grid)
  {
    Html.RenderPartial("_Duc", subitem);
  }
  else
  {
    if (subitem.Grid != null)
    {
      Html.RenderPartial("_Grid", subitem.Grid);
    }
  }
}
